{
    "name": "Simplest Instant Search Replacement",
    "description": "Navigate Google Search results using Tab and Shift+Tab",
    "version": "2.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.google.com/search*"],
        "js": ["contentScript.js"]
      }
    ],
    "manifest_version": 2
  }
  